{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php@0.7.3"
    }
  ],
  "routes": [
    {
      "src": "/build/assets/(.*)",
      "dest": "public/build/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "public/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ]
}